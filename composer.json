{
    "name": "jpcaparas/box-laravel",
    "description": "A service provider for the Box v2 SDK.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "JP Caparas",
            "email": "jp@jpcaparas.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "JPCaparas\\Box\\": "/src"
        }
    },
    "require": {
        "php": ">=7.0.0",
        "illuminate/container": "~5.4.0|~5.5.0",
        "illuminate/support": "~5.4.0|~5.5.0",
        "linkstreet/box-sdk": "dev-develop",
        "symfony/http-foundation": "^3.3"
    },
    "repositories": [
      {
        "type": "vcs",
        "url": "https://github.com/jpcaparas/box-sdk"
      }
    ],
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
